    <SelfContained>false</SelfContained>
    <RuntimeIdentifiers>osx-x64;linux-x64</RuntimeIdentifiers>
    <RootNamespace>Cythral.CloudFormation.ApprovalNotification</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.5.9.4" />
    <PackageReference Include="AWSSDK.StepFunctions" Version="3.5.3.33" />
    <PackageReference Include="Brighid.Identity.Client" Version="0.1.91" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.5.1.48" />
    <PackageReference Include="Lambdajection" Version="$(LambdajectionVersion)" />
    <PackageReference Include="Lambdajection.Encryption" Version="$(LambdajectionVersion)" />
    <PackageReference Include="Lambdajection.Runtime" Version="$(LambdajectionVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AwsUtils\SimpleStorageService\SimpleStorageService.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PreprocessFile Include="..\Common\*.cs" />
  </ItemGroup>
</Project>
