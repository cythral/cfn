<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <SelfContained>false</SelfContained>
    <RuntimeIdentifiers>osx-x64;linux-x64</RuntimeIdentifiers>
    <RootNamespace>Cythral.CloudFormation.S3Deployment</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.5.0" />
    <PackageReference Include="AWSSDK.SecurityToken" Version="3.5.1.22" />
    <PackageReference Include="Octokit" Version="0.47.0" />
    <PackageReference Include="Lambdajection" Version="$(LambdajectionVersion)" />
    <PackageReference Include="Lambdajection.Encryption" Version="$(LambdajectionVersion)" />
    <PackageReference Include="Lambdajection.Runtime" Version="$(LambdajectionVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AwsUtils\SimpleStorageService\SimpleStorageService.csproj" />
    <ProjectReference Include="..\AwsUtils\Common\AwsUtils.Common.csproj" />
    <ProjectReference Include="..\GithubUtils\GithubUtils.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PreprocessFile Include="..\Common\*.cs" />
  </ItemGroup>

  <ItemGroup>
    <InternalsVisibleTo Include="DynamicProxyGenAssembly2" />
  </ItemGroup>
</Project>
