{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": ["test"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
